<div class="container">

  <div class="row">
    <h2>View all items</h2>
  </div>
  <div class="row">
    {{#each items}}
      <div>
          <a href='/users/{{this.owner}}'>{{this.name}}</a>
          <div>
            <img id="itemImage" src="{{this.image}}"/>
          </div>
          <p>The owner of this item is {{this.ownerName}}</p>
      </div>

    {{/each}}
  </div>
</div>
