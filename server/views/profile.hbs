<h1>Welcome {{name}}</h1>

<p>{{name}}</p>
<img src={{image}}/>
<p>{{description}}</p>

<h2>Here are your  <!-- depends on if open or closed--></h2>
{{#each openItems}}
<div class="item">
    <h3>{{this.name}}</h3>
    <img src="{{this.image}}" />
    <p>{{this.description}}</p>
    <p>The owner of this item is {{this.owner}}</p>
</div>
{{/each}}
